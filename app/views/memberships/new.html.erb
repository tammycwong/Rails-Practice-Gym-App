<h1>New Member Form </h1>
<% if flash[:errors] %>
    <% flash[:errors].each do |error|%>
        <p> <%=error%> </p>
    <%end%>
<%end%>
<%= form_for @membership do |f| %>
    <%= f.label :gym %>
    <%= f.collection_select :gym_id, @gyms, :id, :name %>

    <%= f.label :client %>
    <%= f.collection_select :client_id, @clients, :id, :name %>

    <%= f.label :charge %>
    <%= f.number_field :charge %>

    <%= f.submit %>
<% end %>